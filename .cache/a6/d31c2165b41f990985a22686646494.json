if (this.lastModel) {\r\n            const editBtn = document.getElementById(`edit-comment-${model.Id}`);\r\n            if (editBtn) {\r\n                editBtn.addEventListener('click', this.toggleEditing.bind(this));\r\n            }\r\n        }\r\n        if (this.didMountFn)\r\n            this.didMountFn(model);\r\n    }\r\n    toggleEditing() {\r\n        this.editing = !this.editing;\r\n        if (this.lastModel) {\r\n            this.update(this.lastModel);\r\n        }\r\n    }\r\n    didMount(cb) {\r\n        this.didMountFn = cb;\r\n    }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"12e74d67fb357a1b06015c8c5774bb67","cacheData":{"env":{}}}